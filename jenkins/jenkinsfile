pipeline{
 parameters { 
    choice(name: 'BRANCH', choices: ['main', 'REL_INT_1.0', ], description: 'branch') }

stages{
    stage('vcs')
    {
        steps
        git branch: '$BRANCH', url: 'https://github.com/yaminikor/spring-petclinic.git'
    }
    }
    stages{
        stage('build')
{
    sh 'mvn package'
}    }
    
} 

  